<div class="row">
{% for post in postslist | reverse %}
<div class="col-md-4 col-6 p-3">
{% if post.data.image %}<a href="{{ post.url }}"><img class="img-posting img-fluid" alt="{{post.data.title}}" src="{{post.data.image}}"/></a>{% endif %}
<h3 class="blog-list-title mt-3"><a href="{{ post.url }}">{{ post.data.title }}</a></h3>
<p class="blog-list-article ">
{{post.data.description}}
</p>
</div>
{% endfor %}
</div>

